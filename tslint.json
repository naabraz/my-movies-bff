{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "ordered-imports": true,
    "interface-name": [true, "never-prefix"],
    "no-implicit-dependencies": [
      true,
      [
        "_data",
        "_helpers",
        "_interfaces",
        "_resolvers",
        "_schema",
        "_types"
        ]
      ]
  },
  "rulesDirectory": []
}
