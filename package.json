{
  "name": "my-movies-bff",
  "version": "1.0.0",
  "description": "git@github.com:naabraz/my-movies-bff.git",
  "main": "index.js",
  "scripts": {
    "lint": "eslint '**/*.ts'",
    "prepush": "yarn run lint",
    "start:prod": "pm2 start server.ts --name my-movies-bff",
    "stop:prod": "pm2 stop my-movies-bff",
    "start:dev": "yarn run build:dev",
    "build:dev": "nodemon 'server.ts' --exec 'ts-node' server.ts -e ts,graphql",
    "test": "jest",
    "postinstall": "link-module-alias",
    "preinstall": "command -v link-module-alias && link-module-alias clean || true"
  },
  "_moduleAliases": {
    "~root": ".",
    "~data": "src/data",
    "~helpers": "src/helpers",
    "~interfaces": "src/interfaces",
    "~resolvers": "src/resolvers",
    "~schema": "src/schema",
    "~types": "src/types"
  },
  "repository": "git@github.com:naabraz/my-movies-bff.git",
  "author": "Natalia Braz <naabraz@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^3.1.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "ts-jest": "^26.5.6",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@types/compression": "^1.0.1",
    "@types/express": "^4.17.2",
    "@types/graphql": "^14.5.0",
    "@types/node": "^13.7.0",
    "apollo-server": "^2.25.4",
    "apollo-server-express": "^2.9.16",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "graphql-import-node": "^0.0.4",
    "graphql-tools": "^6.0.9",
    "http": "^0.0.0",
    "link-module-alias": "^1.2.0",
    "node-fetch": "^2.6.1"
  }
}
